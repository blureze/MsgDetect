{
	"manifest_version": 2,

	"name": "chrome extention test",
	"description": "This is a practice for Facebook.",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},

	"content_scripts": [
	    {
	        "matches": ["https://*/*","http://*/*"],
	        "js": ["jquery-3.0.0.min.js", "content.js"],
	        "run_at": "document_end"
	    }
	]

	// "background" : {
	// 	"scripts": ["background.js"]
	// }

}